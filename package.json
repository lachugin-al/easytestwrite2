{
  "name": "easytestwrite2",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p .",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "rebuild:sharp": "npm rebuild sharp --force",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --dir tests/e2e",
    "test:e2e:report": "vitest run --dir tests/e2e && allure generate --clean allure-results -o allure-report && allure open allure-report",
    "test:e2e:smoke": "sh -lc 'vitest run tests/e2e/specs/example_allure.test.ts; code=$?; allure generate --clean allure-results -o allure-report || true; allure open allure-report || true; exit $code'",
    "test:e2e:smoke:ci": "sh -lc 'vitest run tests/e2e/specs/example_allure.test.ts; code=$?; allure generate --clean allure-results -o allure-report || true; exit $code'",
    "appium": "appium -p 4723",
    "allure:generate": "allure generate --clean allure-results -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "allure-commandline": "^2.30.0",
    "allure-js-commons": "^3.3.3",
    "allure-vitest": "^3.3.3",
    "appium": "^2.19.0",
    "appium-uiautomator2-driver": "^4.2.8",
    "appium-xcuitest-driver": "^9.10.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "start-server-and-test": "^2.0.8",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "webdriverio": "^9.19.1"
  }
}
